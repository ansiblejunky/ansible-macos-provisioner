#LogLevel DEBUG

# VAGRANT
Host 192.168.*.*
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null

# LAB ENVIRONMENT
Host *.example.com
    StrictHostKeyChecking no
    UserKnownHostsFile /dev/null
    ControlPath ~/.ssh/controlmasters/%r@%h:%p
    ControlMaster auto

# GITLAB
Host gitlab.com
    IdentityFile ~/.ssh/id_rsa_gitlab

# RED HAT
Host *.redhat.com
  GSSAPIAuthentication yes
  GSSAPIDelegateCredentials no
